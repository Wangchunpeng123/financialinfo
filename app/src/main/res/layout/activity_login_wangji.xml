<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.LoginWangjiActivity">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:id="@+id/back_iv"
        android:src="@mipmap/denglu_icon_jiantou"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_30"
        android:padding="@dimen/dp_10"
        />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/denglu_top_bg"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/userzhuche_tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="忘记密码"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="@dimen/dp_30"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:textSize="@dimen/dp_25"
        android:textColor="@color/black"
        />

    <TextView
        android:id="@+id/userzhuche_tv_shoujihao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="手    机    号"
        android:textColor="#ff333333"
        android:textSize="@dimen/sp_15"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userzhuche_tv_title"
        android:layout_marginTop="@dimen/dp_80"
        android:layout_marginLeft="@dimen/dp_20"/>

    <EditText
        android:id="@+id/userzhuche_et_shoujihao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_13"
        android:layout_marginTop="@dimen/dp_80"
        android:background="@null"
        android:hint="请输入手机号"
        android:inputType="number"
        android:maxLength="11"
        android:textSize="@dimen/sp_15"
        app:layout_constraintLeft_toRightOf="@id/userzhuche_tv_shoujihao"
        app:layout_constraintTop_toBottomOf="@id/userzhuche_tv_title" />

    <View
        android:id="@+id/userzhuche_line1"
        android:layout_width="match_parent"
        android:layout_height="12dp"
        android:background="@drawable/bg_denglu_line"
        app:layout_constraintTop_toBottomOf="@id/userzhuche_et_shoujihao"
        android:layout_marginTop="@dimen/dp_13"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="@dimen/dp_20"
        android:layout_marginLeft="@dimen/dp_20"/>
    <TextView
        android:id="@+id/userzhuche_tv_tuxing_yanzhengma"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="图形验证码"
        android:textColor="#ff333333"
        android:textSize="@dimen/sp_15"
        app:layout_constraintTop_toBottomOf="@+id/userzhuche_line1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="@dimen/dp_26"
        android:layout_marginLeft="@dimen/dp_20"/>
    <EditText
        android:id="@+id/userzhuche_et_tuxing_yanzhengma"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@null"
        android:hint="图形验证码"
        android:textSize="@dimen/sp_15"
        android:maxLength="4"
        app:layout_constraintTop_toBottomOf="@+id/userzhuche_line1"
        app:layout_constraintLeft_toRightOf="@id/userzhuche_tv_tuxing_yanzhengma"
        android:layout_marginTop="@dimen/dp_26"
        android:layout_marginLeft="@dimen/dp_13"/>

    <ImageView
        android:id="@+id/userzhuche_iv_tuxing_yanzhengma"
        android:layout_width="@dimen/dp_60"
        android:layout_height="@dimen/dp_30"
        android:scaleType="fitXY"
        android:src="@mipmap/caijingshuju_icon_guoqi_bg"
        app:layout_constraintTop_toBottomOf="@+id/userzhuche_line1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="@dimen/dp_14"
        android:layout_marginRight="@dimen/dp_20" />
    <View
        android:id="@+id/userzhuche_line2"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@drawable/bg_denglu_line"
        app:layout_constraintTop_toBottomOf="@id/userzhuche_tv_tuxing_yanzhengma"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="@dimen/dp_13"
        android:layout_marginRight="@dimen/dp_20"
        android:layout_marginLeft="@dimen/dp_20"/>
    <TextView
        android:id="@+id/userzhuche_tv_shouji_yanzhengma"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="手机验证码"
        android:textColor="#ff333333"
        android:textSize="@dimen/sp_15"
        app:layout_constraintTop_toBottomOf="@+id/userzhuche_line2"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="@dimen/dp_26"
        android:layout_marginLeft="@dimen/dp_20"
        />

    <EditText
        android:id="@+id/userzhuche_et_shouji_yanzhengma"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_13"
        android:layout_marginTop="28dp"
        android:background="@null"
        android:hint="手机验证码"
        android:inputType="number"
        android:textSize="@dimen/sp_15"
        android:maxLength="6"
        app:layout_constraintLeft_toRightOf="@+id/userzhuche_tv_shouji_yanzhengma"
        app:layout_constraintTop_toBottomOf="@+id/userzhuche_line2" />

    <TextView
        android:id="@+id/userzhuche_tv_shouji_yanzhengma_click"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="获取验证码"
        android:textColor="#fffb695d"
        android:textSize="11sp"
        app:layout_constraintTop_toBottomOf="@+id/userzhuche_line2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@+id/userzhuche_tv_shouji_yanzhengma"
        android:layout_marginRight="@dimen/dp_20"/>

    <View
        android:id="@+id/userzhuche_line3"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@drawable/bg_denglu_line"
        app:layout_constraintTop_toBottomOf="@+id/userzhuche_tv_shouji_yanzhengma"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="@dimen/dp_13"
        android:layout_marginRight="@dimen/dp_20"
        android:layout_marginLeft="@dimen/dp_20"
        />
    <TextView
        android:id="@+id/userzhuche_tv_shezhimima"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="重 置 密 码"
        android:textColor="#ff333333"
        android:textSize="@dimen/sp_15"
        app:layout_constraintTop_toBottomOf="@+id/userzhuche_line3"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="@dimen/dp_26"
        android:layout_marginLeft="@dimen/dp_20"/>
    <EditText
        android:id="@+id/userzhuche_et_shezhimima"
        android:layout_width="@dimen/dp_150"
        android:layout_height="wrap_content"
        android:background="@null"
        android:hint="输入密码"
        android:textSize="@dimen/sp_15"
        android:maxLength="16"
        app:layout_constraintTop_toBottomOf="@+id/userzhuche_line3"
        app:layout_constraintLeft_toRightOf="@+id/userzhuche_tv_shezhimima"
        android:layout_marginTop="@dimen/dp_26"
        android:layout_marginLeft="@dimen/dp_13"/>
    <View
        android:id="@+id/userzhuche_line4"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@drawable/bg_denglu_line"
        app:layout_constraintTop_toBottomOf="@+id/userzhuche_tv_shezhimima"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="@dimen/dp_13"
        android:layout_marginRight="@dimen/dp_20"
        android:layout_marginLeft="@dimen/dp_20"/>
    <TextView
        android:id="@+id/userzhuche_tv_qr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="确 认 密 码"
        android:textColor="#ff333333"
        android:textSize="@dimen/sp_15"
        app:layout_constraintTop_toBottomOf="@+id/userzhuche_line4"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="@dimen/dp_26"
        android:layout_marginLeft="@dimen/dp_20"/>
    <EditText
        android:id="@+id/userzhuche_et_qr"
        android:layout_width="@dimen/dp_150"
        android:layout_height="wrap_content"
        android:background="@null"
        android:hint="确认密码"
        android:textSize="@dimen/sp_15"
        android:maxLength="16"
        app:layout_constraintTop_toBottomOf="@+id/userzhuche_line4"
        app:layout_constraintLeft_toRightOf="@+id/userzhuche_tv_qr"
        android:layout_marginTop="@dimen/dp_26"
        android:layout_marginLeft="@dimen/dp_13"/>
    <View
        android:id="@+id/userzhuche_line5"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@drawable/bg_denglu_line"
        app:layout_constraintTop_toBottomOf="@+id/userzhuche_tv_qr"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="@dimen/dp_13"
        android:layout_marginRight="@dimen/dp_20"
        android:layout_marginLeft="@dimen/dp_20"/>

    <ImageView
        android:id="@+id/userzhuche_zhuche"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/white_radius"
        android:scaleType="fitXY"
        android:src="@mipmap/botton_dengluhover"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_20"
        android:layout_marginBottom="@dimen/dp_10"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="确认重置"
        android:textColor="#ffffffff"
        android:textSize="@dimen/sp_15"
        app:layout_constraintTop_toTopOf="@+id/userzhuche_zhuche"
        app:layout_constraintLeft_toLeftOf="@+id/userzhuche_zhuche"
        app:layout_constraintRight_toRightOf="@+id/userzhuche_zhuche"
        app:layout_constraintBottom_toBottomOf="@+id/userzhuche_zhuche"
        />




</androidx.constraintlayout.widget.ConstraintLayout>